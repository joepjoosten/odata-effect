{
  "name": "@odata-effect/odata-effect-generator",
  "version": "0.6.10",
  "type": "module",
  "license": "MIT",
  "description": "Effect-based OData service code generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/joepjoosten/odata-effect.git",
    "directory": "packages/odata-effect-generator"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "bin": {
    "odata-effect-gen": "./dist/esm/bin.js"
  },
  "scripts": {
    "codegen": "build-utils prepare-v2",
    "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "@effect/cli": "^0.73.0",
    "@effect/platform": "^0.94.1",
    "@effect/platform-node": "^0.104.0",
    "effect": "^3.19.14",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/xml2js": "^0.4.14"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
